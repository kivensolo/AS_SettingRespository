<templateSet group="AndroidLog">
  <template name="zld" value="ZLog.d(&quot;$content$&quot;)" description="com.zeke.kangaroo.utils.ZLog.d(&quot;$content$&quot;)" toReformat="true" toShortenFQNames="true">
    <variable name="content" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
      <option name="KOTLIN_STATEMENT" value="true" />
    </context>
  </template>
  <template name="zldt" value="ZLog.d(TAG, &quot;$content$&quot;)" description="ZLog.d(TAG,String)" toReformat="true" toShortenFQNames="true">
    <variable name="content" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
      <option name="KOTLIN_STATEMENT" value="true" />
    </context>
  </template>
</templateSet>