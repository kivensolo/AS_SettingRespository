  </template>
  <template name="include_custom" value="// setting.gradle文件中配置子模块&#10;include ':$MODULE_NAME$'&#10;project(':$MODULE_NAME$').projectDir = new File('$MODULE_PATH$')&#10;&#10;//Copy以下代码至目标module&#10;implementation project(':$MODULE_NAME$')" description="include local module to current module." toReformat="false" toShortenFQNames="true">
