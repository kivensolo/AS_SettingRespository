<application>
  <component name="CommunityFileTypes" version="11">
    <ignoreFiles list="CVS;SCCS;RCS;rcs;.DS_Store;.svn;.pyc;.pyo;*.pyc;*.pyo;.git;*.hprof;_svn;.hg;*.lib;*~;__pycache__;.bundle;*.rbc;" />
    <extensionMap>
      <mapping pattern="test" type="CLASS" />
      <mapping ext="jspx" type="XML" />
      <mapping ext="tagx" type="XML" />
    </extensionMap>
    <setting name="fileTypeChangedCounter" value="1" />
  </component>
  <component name="FileTypeManager" version="18">
    <ignoreFiles list="*.hprof;*.pyc;*.pyo;*.rbc;*~;.DS_Store;.git;.hg;.idea;.svn;CVS;RCS;SCCS;__pycache__;_svn;rcs;vssver.scc;vssver2.scc;" />
    <extensionMap>
      <mapping pattern="activity_ai_testxml" type="AUTO_DETECTED" />
      <mapping ext="gradle.kts" type="Kotlin" />
      <mapping ext="main.kts" type="Kotlin" />
      <removed_mapping ext="md" approved="true" type="Markdown" />
    </extensionMap>
  </component>
</application>